
@{
    ViewBag.Title = "Inicio";
}


<!-- Page Heading -->
<h1 class="h3 mb-2 text-gray-800">« Mantenimiento AFP »</h1>
<p></p>
<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Lista de Agencias</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
                <div class="row" style="width:99%;">
                    <div class="col-sm-12">
                        <table class="table table-bordered dataTable" id="dataTable" width="100%" cellspacing="0"
                               role="grid" aria-describedby="dataTable_info" style="width: 100%;">
                            <thead>
                                <tr>
                                    <th>AFP</th>
                                    <th>Comisión Fija</th>
                                    <th>Comisión Flujo </th>
                                    <th>Comisión Anual</th>
                                    <th>Prima Seguro</th>
                                    <th>Remuneración Max</th>
                                    <th colspan="3" style="text-align: center;">Acciones</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 1; i < 3; i++)
                                {


                                    <tr role="row" class="odd">
                                        <td>AFP Prima</td>
                                        <td>15.20</td>
                                        <td>13.10</td>
                                        <td>23.50</td>
                                        <td>53.12</td>
                                        <td>75.20</td>
                                        <td colspan="3" style="text-align: center;">
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-success">
                                                <span class="glyphicon glyphicon-eye-open"></span>
                                            </a>
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-info">
                                                <span class="glyphicon glyphicon-edit"></span>
                                            </a>
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-danger">
                                                <span class="glyphicon glyphicon-remove"></span>
                                            </a>
                                        </td>
                                    </tr>

                                    <tr role="row" class="odd">
                                        <td>AFP Habitat</td>
                                        <td>15.20</td>
                                        <td>13.10</td>
                                        <td>23.50</td>
                                        <td>53.12</td>
                                        <td>75.20</td>
                                        <td colspan="3" style="text-align: center;">
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-success">
                                                <span class="glyphicon glyphicon-eye-open"></span>
                                            </a>
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-info">
                                                <span class="glyphicon glyphicon-edit"></span>
                                            </a>
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-danger">
                                                <span class="glyphicon glyphicon-remove"></span>
                                            </a>
                                        </td>
                                    </tr>

                                    <tr role="row" class="odd">
                                        <td>AFP Integra</td>
                                        <td>15.20</td>
                                        <td>13.10</td>
                                        <td>23.50</td>
                                        <td>53.12</td>
                                        <td>75.20</td>
                                        <td colspan="3" style="text-align: center;">
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-success">
                                                <span class="glyphicon glyphicon-eye-open"></span>
                                            </a>
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-info">
                                                <span class="glyphicon glyphicon-edit"></span>
                                            </a>
                                            <a href="@Url.Action("Inicio", "AFP")" class="btn-circle btn-sm btn-danger">
                                                <span class="glyphicon glyphicon-remove"></span>
                                            </a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="row" style="width:99%;">
                    <div class="col-sm-12 col-md-9">
                        <a href="@Url.Action("Nuevo", "AFP")" class="btn btn-sm btn-primary shadow-sm">
                            <i class="fas fa-clipboard fa-sm "></i> Nuevo AFP
                        </a>
                    </div>

                    <div class="col-sm-12 col-md-3">
                        <div>
                            <ul class="pagination">
                                <li class="paginate_button page-item previous">
                                    <a href="#" class="page-link">Anterior</a>
                                </li>
                                <li class="paginate_button page-item active">
                                    <a href="#" class="page-link">1</a>
                                </li>
                                <li class="">
                                    <a href="#" class="page-link">2</a>
                                </li>
                                <li class="paginate_button page-item">
                                    <a href="#" class="page-link">3</a>
                                </li>
                                <li class="paginate_button page-item next">
                                    <a href="#" class="page-link">Siguiente</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
